<span style="position:relative;" vg-attr="c.AttrMap">
    <input type="text" autocomplete="off"
           :style="c.EditStyle"
           :node-id="c.editIdStr"
           :value="c.Value"
           @focus="c.onFocus(event)"
           @blur="c.onBlur(event)"
           :onkeydown="c.onKeyDownHookText()"
    />
    <div style="position:absolute; display: none;" :node-id="c.suggestIdStr">
        <div class="text-nowrap overflow-hidden"
             vg-for="idx, suggestion := range c.proposedValues"
             vg-key="idx"
             :style="c.suggestionStyle(idx)"
             vg-content="suggestion"
             @mousedown="c.suggestionClick(event, idx)"></div>
    </div>
</span>