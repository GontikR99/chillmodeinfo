{
  "name": "chillmodeinfo",
  "version": "0.0.10",
  "description": "Chill Mode guild's DKP tracker and status web site",
  "main": "src/electronmain.js",
  "scripts": {
    "start": "electron-forge start",
    "startTransparent": "electron-forge start -- --enable-transparent-visuals --disable-gpu",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GontikR99/chillmodeinfo.git"
  },
  "keywords": [],
  "author": "GontikR99",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GontikR99/chillmodeinfo/issues"
  },
  "homepage": "https://github.com/GontikR99/chillmodeinfo#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "chillmodeinfo"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "11.2.1"
  }
}
